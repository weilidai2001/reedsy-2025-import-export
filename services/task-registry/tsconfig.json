{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "../",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "@reedsy/shared/*": ["../../shared/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "database/in-memory-db.ts",
    "database/job-repository.ts"
  ],
  "references": [{ "path": "../shared" }]
}
